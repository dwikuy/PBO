package looping;

/**
 *
 * @author PCL-B00-00
 */
public class Looping {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {

        for (int i=1; i<=3; i++){
            for (int j=1; j<=3; j++){
              int  x = (i+j)-1;
                System.out.print(x);
            }
              System.out.println( );
        }
    }
    
}
